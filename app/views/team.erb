<div class="team">
  <h1> <%= @team.name %></h1>
  <h2>Team Motto: <%= @team.motto %>!</h2>
</div><br>

<% @members.each do |member| %>
  <div class="course">
    <h1>Hero Name: <%= member.name %></h1><br>
    <p>Hero Power: <%= member.power %></p><br>
    <p>Hero Biography: <%= member.biography %></p><br>
  </div><br>
<% end %>
